html {
	margin: 10px 10px 10px 10px;
	font-family: "Source Sans Pro", "Arial", "Helvetica";
	font-size: 14px;
	line-height: 1.42857;
}

.alert-success {
	color: BLACK;
	border-color: black !important;
	background-color: #8FBC8F;
	background-image: -moz-linear-gradient(top, rgb(155, 164, 23) 0,
		rgb(197, 207, 13) 100%);
	background-image: -ms-linear-gradient(top, rgb(155, 164, 23) 0,
		rgb(197, 207, 13) 100%);
	background-image: -o-linear-gradient(top, rgb(155, 164, 23) 0,
		rgb(197, 207, 13) 100%);
	background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, rgb(155,
		164, 23)), color-stop(100%, rgb(197, 207, 13)));
	background-image: -webkit-linear-gradient(top, rgb(155, 164, 23) 0,
		rgb(197, 207, 13) 100%);
	background-image: linear-gradient(to bottom, rgb(155, 164, 23) 0,
		rgb(197, 207, 13) 100%);
}

.alert-danger {
	color: rgb(250, 250, 250);
	background-image: linear-gradient(to bottom, #CC0000, #FF0000)
		!important;
	border-color: rgb(171, 56, 7) !important;
}

